{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "openAiLocation": {
      "value": "${AZURE_OPENAI_LOCATION=eastus}"
    },
    "chatGptModelName": {
      "value": "${AZURE_OPENAI_CHAT_MODEL=gpt-4o}"
    },
    "chatGptModelVersion": {
      "value": "${AZURE_OPENAI_CHAT_MODEL_VERSION=2024-08-06}"
    },
    "chatGptDeploymentCapacity": {
      "value": "${AZURE_OPENAI_CHAT_DEPLOYMENT_CAPACITY=30}"
    },
    "embeddingModelName": {
      "value": "${AZURE_OPENAI_EMBEDDING_MODEL=text-embedding-3-large}"
    },
    "embeddingModelVersion": {
      "value": "${AZURE_OPENAI_EMBEDDING_MODEL_VERSION=}"
    },
    "embeddingDeploymentCapacity": {
      "value": "${AZURE_OPENAI_EMBEDDING_DEPLOYMENT_CAPACITY=30}"
    },
    "embeddingModelDimensions": {
      "value": "${AZURE_OPENAI_EMBEDDING_DIMENSIONS=3072}"
    },
    "useAIFoundry": {
      "value": "${USE_AI_FOUNDRY=true}"
    },
    "useAuthentication": {
      "value": "${AZURE_USE_AUTHENTICATION=false}"
    },
    "authTenantId": {
      "value": "${AZURE_AUTH_TENANT_ID=}"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID=}"
    }
  }
}
